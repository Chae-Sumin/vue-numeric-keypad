{
  "name": "vue-numeric-keypad",
  "version": "1.2.3",
  "description": "The virtual numeric keypad that can be used on Vue.",
  "main": "dist/vue-numeric-keypad.ssr.js",
  "browser": "dist/vue-numeric-keypad.esm.js",
  "module": "dist/vue-numeric-keypad.esm.js",
  "unpkg": "dist/vue-numeric-keypad.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "demo": "vue-cli-service build dev/serve.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "cross-env": "^7.0.3",
    "gh-pages": "^4.0.0",
    "jsencrypt": "^3.2.1",
    "minimist": "^1.2.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chae-Sumin/vue-numeric-keypad.git"
  },
  "keywords": [
    "vue",
    "vue2",
    "vue.js",
    "random",
    "randomize",
    "virtual",
    "modal",
    "numeric",
    "number",
    "keyboard",
    "keypad",
    "encrypt",
    "encryption",
    "pseudo"
  ],
  "author": "Chae-Sumin(cotnmin)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Chae-Sumin/vue-numeric-keypad/issues"
  },
  "homepage": "https://github.com/Chae-Sumin/vue-numeric-keypad#readme"
}
